{
  "name": "ohlcv",
  "version": "1.0.0",
  "scripts": {
    "db:generate": "sqlite3 db/ohlcv.db3 < db/migration-001.sql",
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile build/elysia-ohlcv-server src/index.ts",
    "serve": "PORT=49999 ./build/elysia-ohlcv-server &!",
    "test": "bun test --env-file=.env.local"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/cron": "^1.4.1",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.6",
    "bun-plugin-tailwind": "^0.1.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "elysia": "latest",
    "hono": "^4.10.6",
    "itty-fetcher": "^1.0.10",
    "node-cache": "^5.1.2",
    "radash": "^12.1.1",
    "tailwindcss": "^4.1.17",
    "tiny-invariant": "^1.3.3",
    "typebox": "^1.0.53",
    "x402-hono": "^0.7.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "bun-types": "latest",
    "daisyui": "^5.5.5"
  },
  "module": "src/index.js"
}
